mysql> select * from lab_priceservice limit 1; - показать
mysql>alter table mobil_organization drop column lab_price_id; удалить поле в таблице
mysql> show tables;
mysql> show create table lab_price; посмотреть табличку
mysql> drop table lab_price_orgs; -удалить таблицу
